<Layout title="Desarrollo Web">
  <main>
    <section
      class="h-[106vh] flex justify-center items-center bg-[url('/imagen_desarrolloweb.webp')] celular:bg-[url('/imagen_desarrollowebcelular.webp')] bg-cover bg-opacity-10"
